{
	"name": "obsidian-svelte-starter",
	"version": "1.0.3",
	"description": "A plugin development template for devs who want to use svelte in Obsidian.",
	"main": "main.js",
	"scripts": {
		"lint": "eslint . --ext .ts",
		"dev": "npm run lint && vite build --mode development --watch",
		"build": "npx svelte-check && vite build --mode production",
		"bumpversion": "node version-bump.mjs && git add manifest.json versions.json",
		"test": "jest",
		"test-int": "jest --watch"
	},
	"keywords": [],
	"author": "Quorafind",
	"devDependencies": {
		"@jest/globals": "^29.5.0",
		"@sveltejs/vite-plugin-svelte": "^1.0.1",
		"@tsconfig/svelte": "^3.0.0",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.32.0",
		"@typescript-eslint/parser": "5.32.0",
		"builtin-modules": "3.3.0",
		"eslint": "^8.21.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"jest": "^29.5.0",
		"obsidian": "^1.4.11",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte": "^3.49.0",
		"svelte-check": "^2.8.0",
		"svelte-preprocess": "^4.10.7",
		"ts-jest": "^29.1.0",
		"tslib": "2.4.0",
		"typescript": "4.7.4",
		"vite": "^3.0.0"
	},
	"dependencies": {
		"chartist": "^1.3.0",
		"chrono-node": "^2.7.0",
		"date-fns": "^2.29.3",
		"date-picker-svelte": "^2.3.0"
	}
}
